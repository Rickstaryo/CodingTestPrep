def solution(common):
    num = common[1] - common[0]
    if common[1] + num == common[2]:
        answer = common[-1] + num
    else:
        num = common[1] // common[0]
        answer = common[-1] * num
    return answer

# Divided into


def solution(common):
    answer = 0
    a, b, c = common[:3]
    if (b-a) == a:
        return common[-1]+(b-a)
    else:
        return common[-1] * (b//a)
    return answer
